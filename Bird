        this.type=type;
    }
    public String getType(){
        return this.type;
    }
    public void fly(){
        System.out.println("Iâ€™m a bird who flaps wings to fly.");
    }
    public String toString(){
        return "Bird [type=" + type + "]: ";
    }
}
